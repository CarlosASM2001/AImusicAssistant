{
  "manifest_version": 3,
  "name": "AI Artist Finder",
  "version": "0.1.0",
  "action": { "default_popup": "popup.html", "default_title": "AI Artist Finder" },
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://ai-music-assistant.vercel.app/*"],
  "background": { "service_worker": "src/background.js", "type": "module" }
}